{
  "name": "nodejs-stater",
  "version": "1.0.0",
  "description": "nodejs starter with express and typescript setup",
  "main": "./dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "concurrently \"tsc -w\" \"npm:nodemon\"",
    "nodemon": "nodemon -w dist dist/server.js",
    "genJwtKeys": "scripts/genJwtKeys.sh",
    "migrate": "knex migrate:latest --knexfile dist/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile dist/db/knexfile.js"
  },
  "keywords": [],
  "author": "Ajay Singh",
  "license": "ISC",
  "imports": {
    "#src/*": "./dist/*"
  },
  "dependencies": {
    "@types/passport-jwt": "^3.0.7",
    "connect-session-knex": "^3.0.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
