{
  "name": "nodejs-stater",
  "version": "1.0.0",
  "description": "nodejs starter with express and typescript setup",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "engineStrict": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/app.js",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && concurrently \"npm:tsc\" \"npm:tsc-alias\" \"npm:nodemon\"",
    "tsc": "tsc -w",
    "tsc-alias": "tsc-alias -w",
    "nodemon": "nodemon -w dist ./dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.14",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "knex": "^2.3.0",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "tsc-alias": "^1.7.1"
  }
}
